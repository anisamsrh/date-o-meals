{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style":"off",
        "camelcase":"off",
        "max-len":"off",
        "no-underscore-dangle":"off",
        "no-alert":"off",
        "no-console":"off"
    }
}
